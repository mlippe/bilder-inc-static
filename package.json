{
  "name": "bilder-inc-static",
  "scripts": {
    "build": "npm run sass && eleventy",
    "sass": "node-sass -o dist/css --output-style compact src/scss",
    "serve": "npm-run-all sass --parallel watch:*",
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "npm run sass -- --watch",
    "start": "npm run serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "sass": "^1.35.2"
  }
}
